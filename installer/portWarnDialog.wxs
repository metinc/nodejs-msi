<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>

    <!-- port is being used warning dialog -->
    <UI>
      <Dialog Id="PortWarnDlg" Width="270" Height="105" Title="Port Warning" NoMinimize="yes">
        <Control Id="Text" Type="Text" X="38" Y="8" Width="240" Height="40" TabSkip="no" Text="Port [PORT] is already being used by [PORT_BUSY]">
        </Control>
        <Control Id="Next" Type="PushButton" X="147" Y="80" Width="56" Height="17" Default="yes" Text="!(loc.WixUIIgnore)">
        </Control>
        <Control Id="Back" Type="PushButton" X="67" Y="80" Width="56" Height="17" Cancel="yes" Text="!(loc.WixUIBack)">
        </Control>
      </Dialog>
    </UI>
    
    </Fragment>
</Wix>